{
  "name": "deep-todomvc-frontend-test",
  "version": "0.0.1",
  "description": "Description of deep-todomvc-frontend-test",
  "scripts": {
    "preinstall": "cd ../../frontend/js/ && npm install && cd ../../tests/frontend/",
    "postinstall": "jspm install",
    "test": "karma start config.karma.js",
